{
  "name": "react-ui-scaffold",
  "main": "./index.js",
  "version": "1.1.1",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "bootstrap": "^5.0.0",
    "react-bootstrap": "^1.6.0"
  },
  "scripts": {
    "copy-files": "copyfiles -u 1 lib/**/*.css dist",
    "post-build": "rimraf dist/ && tsc && yarn copy-files",
    "build": "tsc -p . && npm run post-build && cp package.json dist && cp README.md dist",
    "deploy": "yarn version && npm run build && cd dist && npm publish",
    "sandbox": "cd sandbox && npm start"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/bootstrap": "^5.0.16",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.20.15",
    "@types/react": "^17.0.0",
    "@types/react-bootstrap": "^0.32.25",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-slider": "^1.3.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "copyfiles": "^2.4.1",
    "eslint": "^7.28.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "rimraf": "^3.0.2",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "typescript": "^4.3.4"
  }
}
